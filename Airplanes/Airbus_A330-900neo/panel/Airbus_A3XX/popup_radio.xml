        <MouseArea id="MouseArea">
            <FloatPosition>0.000,0.000</FloatPosition>
            <Size>246,296</Size>
            <MouseArea id="Active - Standby Swap">
                <FloatPosition>112.000,23.000</FloatPosition>
                <Size>22,20</Size>
                <CursorType>Hand</CursorType>
                <MouseClick id="MouseClick">
                    <Script>
						(M:Event) &apos;LeftSingle&apos; scmp 0 == if{ 1 (&gt;L:ActStby Pressed,bool) 
						(L:Radio Tune Select, number) 0 == if{ 1 (&gt;K:COM_STBY_RADIO_SWAP) g0 }
						(L:Radio Tune Select, number) 1 == if{ 1 (&gt;K:COM2_RADIO_SWAP) g0 }
						(L:Radio Tune Select, number) 2 == if{ 1 (&gt;K:NAV1_RADIO_SWAP) g0 }
						(L:Radio Tune Select, number) 3 == if{ 1 (&gt;K:NAV2_RADIO_SWAP) g0 }
						(L:Radio Tune Select, number) 4 == if{ 1 (&gt;K:ADF1_RADIO_SWAP) g0 }
						:0
						} 
						(M:Event) &apos;LeftRelease&apos; scmp 0 == if{ 0 (&gt;L:ActStby Pressed,bool) }
					</Script>
                    <ClickType>LeftSingle+LeftRelease</ClickType>
                </MouseClick>
                <Tooltip id="Tooltip">
                </Tooltip>
